{
  "name": "drupal/jw_preload",
  "description": "A Drupal 7 module for preloading JW video metadata on the server-side to reduce loading time on the client-side.",
  "type": "drupal-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Donovan Mueller",
      "email": "th@donut.codes"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/RightThisMinute/jw-platform-php.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/RightThisMinute/extra_log-drupal.git"
    }
  ],
  "require": {
    "lstrojny/functional-php": "^1.11",
    "rightthisminute/extra_log-drupal": "7.x-dev",
    "rightthisminute/jw-platform-php": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\jw_preload\\": "src"
    },
    "files": [
      "src/base.php"
    ]
  }
}
